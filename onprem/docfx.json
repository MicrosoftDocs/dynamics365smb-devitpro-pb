{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "../**",
          "**/includes/**",
           "**/_themes/**",
           "**/_themes.pdf/**",
           "README.md",          
           "LICENSE",
           "LICENSE-CODE",
           "ThirdPartyNotices" 
        ]
      }
    ],
    "resource": [
      {
